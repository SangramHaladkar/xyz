<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#ffffffff"
    android:orientation="vertical">

    <com.mobifilia.custom.IconTextView
        android:id="@+id/titleTextView"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:scaleType="fitCenter"
        android:text="Sample"
        android:textColor="@color/colorM3signin" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_below="@+id/titleTextView"
        android:background="@color/color_standard_green" />

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/titleTextView"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:layout_centerHorizontal="true"
        >
        <com.mobifilia.custom.IconTextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="Current Interest Rate"
            android:gravity="left"
            android:textColor="#666666"
            android:textSize="13sp" />

        <com.mobifilia.custom.IconTextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="5dp"
            android:text="="
            android:textColor="#666666"
            android:textSize="15sp" />
        <com.mobifilia.custom.IconTextView
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Current Prime Rate"
            android:gravity="left"
            android:textColor="#666666"
            android:textSize="13sp" />
        <com.mobifilia.custom.IconTextView
            android:id="@+id/signOneTextView"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="-"
            android:layout_marginTop="5dp"
            android:gravity="center_horizontal"
            android:textColor="#666666"
            android:textSize="15sp" />

        <com.mobifilia.custom.IconTextView
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text="Prime Rate Adjustment "
            android:textColor="#666666"
            android:textSize="13sp" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout1"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:layout_centerHorizontal="true"
        >
        <com.mobifilia.custom.IconTextView
            android:id="@+id/ActualCurrentInterestTextView"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="3.00%"
            android:gravity="center_horizontal"
            android:textColor="@color/color_black"
            android:textSize="15sp" />

        <com.mobifilia.custom.IconTextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:text="="
            android:textColor="@color/color_black"
            android:textSize="15sp" />
        <com.mobifilia.custom.IconTextView
            android:id="@+id/ActualPrimeRateTextView"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="3.25%"
            android:gravity="center_horizontal"
            android:textColor="@color/color_black"
            android:textSize="15sp" />
        <com.mobifilia.custom.IconTextView
            android:id="@+id/signTwoTextView"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="-"
            android:gravity="center_horizontal"
            android:textColor="@color/color_black"
            android:textSize="15sp" />

        <com.mobifilia.custom.IconTextView
            android:id="@+id/ActualPrimeRateAdjstumentTextView"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:text="0.25% "
            android:textColor="@color/color_black"
            android:textSize="15sp" />


    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout2"
        android:layout_centerHorizontal="true">


        <com.mobifilia.custom.IconButton
            android:id="@+id/btn_okay"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:background="@drawable/button_rounded"
            android:paddingBottom="10dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="10dp"
            android:text="Okay"
            android:textColor="@color/color_standard_green"
            android:textSize="14sp" /></LinearLayout>

    <com.mobifilia.custom.IconTextView
        android:id="@+id/closeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="7dp"
        android:gravity="right"
        android:paddingRight="14dp"
        android:text="@string/alert_close_button"
        android:textColor="@color/colorM3signin"
        android:textSize="20dp" />

</RelativeLayout>

